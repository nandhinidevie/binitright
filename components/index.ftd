-- ds.copy-small: ©2025 Nandhini. All Rights Reserved.

-- end: ds.section-column

-- end: ftd.mobile

-- end: ftd.column

-- end: footer




-- component typo:
optional caption title:
optional body body:
boolean underline: true
ftd.resizing width: hug-content

-- ds.column:
align-content: left
inset: $ds.spaces.inset-square.zero

-- ftd.text: $typo.title
if: { typo.title != NULL }
color: $ds.colors.text-strong
role: $inherited.types.heading-medium
border-bottom-width.px if { typo.underline }: 1
border-color if { typo.underline }: $inherited.colors.border
width if { typo.underline }: fill-container

-- ds.copy-regular:
if: { typo.body != NULL }
style: light
width if { typo.underline }: fill-container

$typo.body

-- end: ds.column

-- end: typo




-- component typo-small:
optional caption title:
optional body body:

-- ds.column:
inset: $ds.spaces.inset-square.zero
align-content: left
;; spacing: $ds.spaces.vertical-gap.extra-small

-- ftd.text: $typo-small.title
if: { typo-small.title != NULL }
color: $ds.colors.text-strong
role: $inherited.types.heading-small

-- ds.copy-regular:
if: { typo-small.body != NULL }
style: light

$typo-small.body

-- end: ds.column

-- end: typo-small

