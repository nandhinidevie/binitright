-- integer $expanded-tile: 0
-- integer $review-tile: 0

-- string list $selected-item-list:

-- integer $current-slide: 1

-- string $selected-item: Type of Roles Predefined 

-- boolean $show-options: false

-- ftd.image-src site-logo: https://i.postimg.cc/XNksQP3b/fav-icon-bir.png

-- pr.sitemap-data sitemap:
$processor$: pr.sitemap







-- component page:
optional caption title:
ftd.image-src site-logo: $site-logo
pr.toc-item list toc: $sitemap.toc
pr.toc-item list sections: $sitemap.sections
children children:

-- ds.page: $page.title
inset: $ds.spaces.inset-square.zero
margin: $ds.spaces.vertical-gap.zero

-- lets-ga.setup:

-- ds.section-column:
align-content: top-left
inset: $ds.spaces.inset-square.zero
;; inset: $ds.spaces.inset-wide.medium-zero
;; inset: $ds.spaces.inset-wide.large-zero
;; inset: $ds.spaces.inset-tall.zero-medium
margin: $ds.spaces.vertical-gap.zero

/-- ds.header:
sections: $page.sections
site-logo: $page.site-logo

-- ds.column:
content: $page.children
inset: $ds.spaces.inset-wide.medium-zero
margin: $ds.spaces.vertical-gap.zero

-- end: ds.column

-- footer:

-- end: ds.section-column

-- end: ds.page

-- end: page







-- component rustic:
optional caption title:
body body: 
ftd.image-src left-logo: $assets.files.assets.left-quote.svg
ftd.image-src right-logo: $assets.files.assets.right-quote.svg

-- ftd.column:
margin-top.px: 24
margin-bottom.px: 24
align-self: center
width: fill-container

-- ftd.image:
src: $rustic.left-logo
height.fixed.px: 69
width.fixed.px: 69
anchor: parent
top.px: -12
left.px if { ftd.device == "mobile" }: 0
left.px: 0
z-index: 0

-- ftd.column:
align-self: center
padding-vertical.px: 8
padding-horizontal.px: 8
max-width.fixed.percent if { ftd.device == "desktop" }: 90
z-index: 1

-- ftd.text:
color: $inherited.colors.text
role: $inherited.types.heading-small
padding-horizontal.px: 32
margin-top.px: 16
margin-bottom.px: 16
text-align: center
margin-left.px if { ftd.device == "mobile" }: 32
margin-right.px if { ftd.device == "mobile" }: 32

$rustic.body

-- ftd.text: $rustic.title
if: { rustic.title != NULL }
align-self: center
role: $inherited.types.copy-small
color: $inherited.colors.text
padding-horizontal.px: 32

-- end: ftd.column

-- ftd.image:
src: $rustic.right-logo
height.fixed.px: 69
width.fixed.px: 69
anchor: parent
right.px: 0
bottom.px: 0

-- end: ftd.column

-- end: rustic




